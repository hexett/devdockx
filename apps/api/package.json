    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.27.0",
    "chalk": "^5.3.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "express": "^4.18.2",
    "log-symbols": "^5.1.0",
    "stripe": "^11.16.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.1",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.6"
  }
}
