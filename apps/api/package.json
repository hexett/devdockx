{
  "name": "devdockx-api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec \"node --loader ts-node/esm\" --exitcrash src/server.ts",
    "dev:plain": "node --loader ts-node/esm src/server.ts",
